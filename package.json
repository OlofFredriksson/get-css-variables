{
  "name": "get-css-variables",
  "version": "1.1.0",
  "description": "Import CSS variables into a JavaScript Object",
  "keywords": [
    "css"
  ],
  "homepage": "https://github.com/OlofFredriksson/get-css-variables#readme",
  "bugs": {
    "url": "https://github.com/OlofFredriksson/get-css-variables/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlofFredriksson/get-css-variables.git"
  },
  "license": "MIT",
  "author": "Olof",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "bin": "./bin/get-css-variables.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prebuild": "tsc",
    "build": "node build.mjs",
    "postbuild": "dts-bundle-generator -o dist/types/index.d.ts temp/types/index.d.ts",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "run-s eslint prettier:check",
    "lint:fix": "run-s eslint:fix prettier:write",
    "prepack": "release-prepack --bundle",
    "postpack": "release-postpack",
    "prepublishOnly": "release-prepublish --bundle",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "postpublish": "release-postpublish",
    "test": "jest",
    "postversion": "git push --follow-tags"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,json,md,css}": "prettier --write",
    "package.json": "npx sort-package-json -c"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "dependencies": {
    "@adobe/css-tools": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@forsakringskassan/commitlint-config": "3.2.1",
    "@forsakringskassan/eslint-config": "13.2.2",
    "@forsakringskassan/eslint-config-cli": "11.9.11",
    "@forsakringskassan/eslint-config-jest": "11.9.14",
    "@forsakringskassan/prettier-config": "3.1.7",
    "@html-validate/release-scripts": "7.1.0",
    "@tsconfig/node20": "20.1.6",
    "@types/node": "20.19.24",
    "babel-jest": "30.2.0",
    "dts-bundle-generator": "9.5.1",
    "esbuild": "0.25.12",
    "jest": "30.2.0",
    "npm-pkg-lint": "4.2.0",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 10.0.0"
  }
}
