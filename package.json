  },
  "license": "MIT",
  "author": "Olof",
  "main": "dist/index.js",
  "bin": "./bin/get-css-variables.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "prebuild": "tsc",
    "build": "babel src/index.js --out-file dist/index.js",
    "postbuild": "dts-bundle-generator -o dist/index.d.ts temp/types/index.d.ts",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "run-s eslint prettier:check",
    "lint:fix": "run-s eslint:fix prettier:write",
